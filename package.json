{
	"name": "skelekit-wagmiconnect",
	"version": "1.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"docker:build": "docker build -t sk-wc-w-sk-tw-image .",
		"docker:run": "docker run -d --name sk-wc-w-sk-tw-container -p 5173:5173 sk-wc-w-sk-tw-image",
		"docker:stop": "docker stop sk-wc-w-sk-tw-container",
		"docker:remove": "docker rm sk-wc-w-sk-tw-container",
		"docker:clean": "docker rmi -f sk-wc-w-sk-tw-image"
	},
	"devDependencies": {
		"@types/eslint": "8.56.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"postcss": "8.4.36",
		"autoprefixer": "10.4.18",
		"tailwindcss": "3.4.1",
		"@skeletonlabs/skeleton": "2.9.0",
		"@skeletonlabs/tw-plugin": "0.3.1",
		"vite-plugin-tailwind-purgecss": "0.2.0",
		"@tailwindcss/typography": "0.5.10",
		"@tailwindcss/forms": "0.5.7",
		"@types/node": "20.11.29",
		"@sveltejs/adapter-auto": "3.1.1",
		"@sveltejs/kit": "2.5.0",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@typescript-eslint/eslint-plugin": "6.0.0",
		"@typescript-eslint/parser": "6.0.0",
		"svelte": "4.2.9",
		"svelte-check": "3.6.0",
		"tslib": "2.4.1",
		"typescript": "5.3.3",
		"vite": "5.0.12",
		"ip": ">=1.1.9"
	},
	"dependencies": {
		"@floating-ui/dom": "1.6.3",
		"@tanstack/react-query": "^5.18.1",
		"@wagmi/connectors": "4.1.13",
		"@wagmi/core": "2.6.4",
		"@web3modal/wagmi": "4.0.0",
		"svelte-french-toast": "1.2.0",
		"viem": "2.7.6"
	},
	"author": {
		"name": "Elliott Green",
		"url": "https://github.com/Elliott-Green/"
	}
}
